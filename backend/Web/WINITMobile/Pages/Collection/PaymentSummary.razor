        <div class="cls_pay_summary_3_left"><label class="tc_cls">@Localizer["total_collection"]</label></div>
        <div class="cls_pay_summary_3_right">
            <p>@Localizer["inr"]</p><h3>@TotalAmount</h3>
        </div>


    </div>



