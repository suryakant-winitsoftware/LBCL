    }

    .col-6 {
        flex: 1; /* Equal width distribution for divs */
        display: flex;
        align-items: center;
        gap: 0.5rem; /* Optional spacing between label and input */
    }

    .search-icon {
        color: #fff;
        background-color: #fff;
        padding: 10px;
        border-radius: 0 5px 5px 0;
