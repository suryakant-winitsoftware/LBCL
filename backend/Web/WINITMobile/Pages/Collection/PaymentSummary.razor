                                @if (list.Category != "Cash" && list.Category != "CREDITNOTE")
                                {
                                    <div class="cpo_cls d-flex flex-row">
                                        <div class="hdr_cls">
                                            <p>@Localizer["cheque_no"]</p>
                                            <p>@Localizer["branch_name"]</p>
                                            <p>@Localizer["bank_name"]</p>
                                        </div>
                                        <div class="bdy_cls">
                                            <p>: @CPODetails.FirstOrDefault().ChequeNo</p>
                                            <p>: @CPODetails.FirstOrDefault().Branch</p>
                                            <p>: @CPODetails.FirstOrDefault().BankUID</p>
                                        </div>
                                    </div>
                                }
                            }
                            @if (list.flag)
                            {
                                <div class="card1 d-flex flex-row extra_cls">
                                    <div class="col-3" style="margin-left: 4px;">
                                        @Localizer["invoice_no"]
                                    </div>
                                    <div class="col-5">
                                    </div>
                                    <div class="col-2">
                                    </div>
                                    <div class="col-2">
                                        @Localizer["amount"]
                                    </div>
                                </div>
                            }
                        </div>
                    </div>
                    @if (list.IsExpanded)
                    {
                        <div class="card-c extra_sub_cls">
                            @foreach (var c in AllotmentList)
                            {
                                <div class="d-flex flex-row" style="font-size:11px;margin-left: 4px;">
                                    <div class="col-3" style="color:deepskyblue">
                                        @c.ReferenceNumber
                                    </div>
                                    <div class="col-5">
                                    </div>
                                    <div class="col-2">
                                    </div>
                                    <div class="col-2" style="color:deepskyblue">
                                        @_appUser.OrgCurrencyList.FirstOrDefault(p => p.UID == "INR").Symbol
                                        @c.Amount
                                    </div>
                                </div>
                            }
                        </div>
                    }
                }

            }
            else
            {
                <div style="margin-bottom: 3px;"></div>
                <div class="container">
                    <div class="row">
                        <div class="card">
                            <div class="card-title" style="color:red;margin-left:100px;margin-top: 10px;font-size: 12px;">@Localizer["no_records_available"]</div>
                        </div>
                    </div>
                </div>
            }
        }
    </div>

</div>