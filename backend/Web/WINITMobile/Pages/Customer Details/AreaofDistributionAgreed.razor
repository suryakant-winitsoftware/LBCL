<div style="width:100%;float:left;margin:10px 0px 20px 0px;">
    <div class="row mt-3">
        <div class="col-12 d-flex justify-content-start">
            <div class="form-check d-flex cls_flex_align_start" style="width:100%;">
                <input type="checkbox" checked="@_onBoardCustomerDTO.StoreAdditionalInfoCMI.IsAgreedWithTNC" disabled="@isDisabled" @onchange="OnCheckboxChanged" style="width:auto;margin-top:4px;" />
                <label for="termsCheckbox" class="form-check-label mb-0" style="margin-left:8px;">
                    <button type="button" class="btn btn-link p-0 text-decoration-underline text-primary" style="font-size:13px;font-weight:700;color:#1F528E" @onclick="OpenPopup">The Applicant Acknowledges & Agrees with Terms and Conditions</button>.
                </label>
            </div>
        </div>
    </div>
</div>
@if (showErrorPopup)
{
    <div class="popup-overlay">
        <div class="popup-container">
            <div class="popup-header">
                <h3>Error</h3>
                <button class="popup-close" @onclick="OnOKErrorPopup">×</button>
            </div>
            <hr class="popup-divider" />
            <div class="popup-body">
                <p>Please Read the Terms and Conditions Before Saving.</p>
            </div>
            <hr class="popup-divider" />
            <div class="popup-footer">
                <button class="btn btn-primary" @onclick="OnOKErrorPopup">Ok</button>
            </div>
        </div>
    </div>
}
