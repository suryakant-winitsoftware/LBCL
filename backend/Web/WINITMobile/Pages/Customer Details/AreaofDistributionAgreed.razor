@inherits WINITMobile.Pages.Base.BaseComponentBase
<div style="width:100%;float:left;margin:0px 0px;">
    @if (!string.IsNullOrWhiteSpace(ValidationMessage))
    {
        <div class="alert alert-danger">
            <div class="col-md-12">
                <div style="color: red;">@ValidationMessage</div>
            </div>
        </div>
    }
    <div class="card">
        <div class="card-body">
            <div class="cls_cust_information">
                <div class="row">
                    <div class="col-12 col-sm-12 col-md-12 col-lg-12 mb-12">
                        <h6 class="cls_copy_adress_span">Expected T/O with CMI(Rs in Lakhs)</h6>
                    </div>
                    <div class="col-12 col-sm-6 col-md-4 col-lg-3 mb-4">
                        <label class="cls_customer_label">3 Months<span class="cls_section_div_purchase2_span">*</span></label>
                        <input autocomplete="off" class="cls_select2_button @(string.IsNullOrWhiteSpace(_onBoardCustomerDTO.StoreAdditionalInfoCMI?.AodaExpectedTo1.ToString()) && IsSaveAttempted ? "is-invalid" : "")" type="number" placeholder="Enter here" @bind="_onBoardCustomerDTO.StoreAdditionalInfoCMI.AodaExpectedTo1" />
                    </div>
                    <div class="col-12 col-sm-6 col-md-4 col-lg-3 mb-4">
                        <label class="cls_customer_label"> 6 Months<span class="cls_section_div_purchase2_span">*</span></label>
                        <input autocomplete="off" class="cls_select2_button @(string.IsNullOrWhiteSpace(_onBoardCustomerDTO.StoreAdditionalInfoCMI?.AodaExpectedTo2.ToString()) && IsSaveAttempted ? "is-invalid" : "")" type="number" placeholder="Enter here" @bind="_onBoardCustomerDTO.StoreAdditionalInfoCMI.AodaExpectedTo2" />
                    </div>

                    <div class="col-12 col-sm-6 col-md-4 col-lg-3 mb-4">
                        <label class="cls_customer_label"> 1 year<span class="cls_section_div_purchase2_span">*</span></label>
                        <input autocomplete="off" class="cls_select2_button @(string.IsNullOrWhiteSpace(_onBoardCustomerDTO.StoreAdditionalInfoCMI?.AodaExpectedTo3.ToString()) && IsSaveAttempted ? "is-invalid" : "")" type="number" placeholder="Enter here" @bind="_onBoardCustomerDTO.StoreAdditionalInfoCMI.AodaExpectedTo3" />
                    </div>
                    <div class="col-12 col-sm-12 col-md-12 col-lg-12 mb-12">
                        <h6 class="cls_copy_adress_span">Infrastructure Dedicated for CMI</h6>
                    </div>

                </div>
            </div>
        </div>
    </div>
    <div class="card">
        <div class="card-body">
            <div class="cls_cust_information">
                <div class="row">
                    @foreach (var category in radioCategories)
                    {
                        <div class="col-12 col-sm-6 col-md-4 col-lg-3 mb-4">
                            <label class="cls_customer_label cls_mar_btm_10px">@category.Label</label>
                            <div class="cls_btn_y_n">
                                <input type="radio" id="@category.IdYes" name="@category.Name" value="yes" @onchange="() => OnRadioChanged1(category.Key, true)" checked="@category.IsYesChecked" />
                                <label for="@category.IdYes" class="mr-2">Yes</label>
                            </div>
                            <div class="cls_btn_y_n">
                                <input type="radio" id="@category.IdNo" name="@category.Name" value="no" @onchange="() => OnRadioChanged1(category.Key, false)" checked="@category.IsNoChecked" />
                                <label for="@category.IdNo">No</label>
                            </div>
                        </div>
                    }
                </div>
            </div>
        </div>
    </div>
</div>
